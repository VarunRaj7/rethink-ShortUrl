{
  "name": "rethink-shorturl",
  "version": "1.0.0",
  "description": "Rethink ShortURL",
  "main": "server.js",
  "scripts": {
    "start": "node .",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "clean": "rm -rf www/ || true",
    "build": "npm run clean && tsc && cp -rf src/config www/ && cp .npmrc www/.npmrc && cp package.json www/package.json && cp -rf .ebextensions/ www/ && cd www && zip -r Archive.zip . && cd ..",
    "test": "ts-mocha ./test/shortUrl.test.ts"
  },
  "author": "Varun Raj Rayabarapu",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.758.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "is-url": "^1.2.4",
    "mocha": "^8.1.3",
    "nanoid": "^3.1.12",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.13",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/is-url": "^1.2.28",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  }
}
